        "columns": [
            { 'data': 'StudentName' },
            { 'data': 'Address' },
            { 'data': 'Class' },
            { 'data': 'Age' },
            { 'data': 'Hobbies' },
            { 'data': 'Gender' },
            { 'data': 'State' },
            { 'data': 'City' },
            //{ 'data': 'Pincode' }
            //{ 'data': 'Photo' }
