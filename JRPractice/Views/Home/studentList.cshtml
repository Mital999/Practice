    <thead>
        <tr>
            <th>Student Name</th>
            <th>Address</th>
            <th>Class</th>
            <th>Age</th>
            <th>Hobbies</th>
            <th>Gender</th>
            <th>State</th>
            @*<th>City</th>
            <th>Pincode</th>*@
        </tr>
    </thead>
    <tbody></tbody>
</table>
